{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true,
    "commonjs": true
  },
  "rules": {
    "semi": 2,
    "no-console": 0,
    "eqeqeq": ["error", "smart"],
    "max-len": [1, 200, 2],
    "no-var": 2,
    "new-cap": 1,
    "camelcase": 1,
    "no-shadow": 0,
    "prefer-const": 0,
    "no-param-reassign": 1,
    "arrow-body-style": 0,
    "object-shorthand": 0,
    "no-new": 1,
    "indent": [1, 2, { "SwitchCase": 1, "VariableDeclarator": 1 }]
  },
  "globals": {
    "$": false,
    "_": true,
    "cxm": false
  }
}